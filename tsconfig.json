{
    "compilerOptions": {
      "lib": ["dom", "dom.iterable", "esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "noEmit": true,
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "bundler",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "incremental": true,
      "plugins": [
        {
          "name": "next"
        }
      ],
      "baseUrl": ".",
      "paths": {
        "@/*": [
          "./src/*"
        ],
        "react": [ "./node_modules/@types/react" ]
      }
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "**/*.js", "**/*.mjs", ".next/types/**/*.ts", "*.js", "*.mjs", "src/components/experience-form/options-initial-date.js", "src/app/download/cv-components/experience.jsx", "src/app/download/cv-components/education.jsx", "src/app/download/cv-components/header-info.jsx"],
    "exclude": ["node_modules"]
  }